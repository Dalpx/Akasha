RewriteEngine On

# Establece la ruta base para la reescritura
RewriteBase /akasha/server-akasha/src/

# Si el archivo o directorio existe, no hagas nada
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Redirige todo lo dem√°s a index.php
RewriteRule ^(.*)$ index.php [QSA,L]